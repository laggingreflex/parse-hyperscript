    "classnames": "^2.2.5"
  },
  "devDependencies": {
    "buble": "^0.12.5",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^2.3.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "snazzy": "^4.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "xyz": "^0.5.0"
  }
}
